extends layout

block content
    .jarallax#mainpage
        #myCarousel.carousel.slide(data-ride='carousel')
            // Indicators
            ol.carousel-indicators
                li.active(data-target='#myCarousel', data-slide-to='0')
                li(data-target='#myCarousel', data-slide-to='1')
                li(data-target='#myCarousel', data-slide-to='2')
                li(data-target='#myCarousel', data-slide-to='3')
                li(data-target='#myCarousel', data-slide-to='4')
            // Wrapper for slides
            .carousel-inner(role='listbox')
                .item.active
                    img(src='./images/sliderImages/image1.jpg', alt='Chania')
                    .carousel-caption
                        p Web Development through Bootstrap workshop
                        img(src="./images/sliderImages/MS_logo.png")
                .item
                    img(src='./images/sliderImages/image2.jpg', alt='MPSlife')
                    .carousel-caption
                        p Presenting as an MSP
                .item
                    img(src='/images/sliderImages/image3.jpg', alt='AndroidWear')
                    .carousel-caption
                        p AndroidWear workshop 
                .item
                    img(src='/images/sliderImages/image4.jpg', alt='GEIntern')
                    .carousel-caption
                        p While interning at GE 
                .item
                    img(src='/images/sliderImages/image5.jpg', alt='Skydiving')
                    .carousel-caption
                        p Moments after sky-diving 
            // Left and right controls
            a.left.carousel-control(href='#myCarousel', role='button', data-slide='prev')
                span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
                span.sr-only Previous
            a.right.carousel-control(href='#myCarousel', role='button', data-slide='next')
                span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
                span.sr-only Next
        
    #nav.navbar.navbar-custom
        .container-fluid
            .navbar-header
                button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target="#collapsedMenu",aria-expanded="false") 
                    span.glyphicon.glyphicon-list
                    span.sr-only Toggle Navigation
                    span.icon-bar
                    span.icon-bar
                    
                a.navbar-brand(data-id="#mainpage") Sujil Maharjan
            .collapse.navbar-collapse#collapsedMenu
                ul.nav.navbar-nav.navigate-menu
                    li 
                        a.hvr-sweep-to-bottom.active(data-id="#intro") Introduction
                            span.sr-only 
                    li 
                        a.hvr-sweep-to-bottom(data-id="#experience") Portfolio
                    li 
                        a.hvr-sweep-to-bottom(data-id="#skills") Skills
                    li 
                        a.hvr-sweep-to-bottom(data-id="#extracurricular") Leadership

